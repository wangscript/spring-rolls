CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE T_SYSTEM_USER_INFO(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,USERNAME VARCHAR(50) NOT NULL,PASSWORD VARCHAR(50) NOT NULL,CNNAME VARCHAR(50),EMAIL VARCHAR(50),ENABLED INTEGER,ACCOUNT_EXPIRED INTEGER,ACCOUNT_LOCKED INTEGER,CREDENTIALS_EXPIRED INTEGER)
CREATE MEMORY TABLE T_SYSTEM_ROLE_INFO(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NAME VARCHAR(50) NOT NULL,CNNAME VARCHAR(50))
CREATE MEMORY TABLE T_SYSTEM_RESOURCE_INFO(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NAME VARCHAR(50) NOT NULL,PATH VARCHAR(200) NOT NULL,CNNAME VARCHAR(50))
CREATE MEMORY TABLE T_SYSTEM_USER_ROLE(USER_ID INTEGER,ROLE_ID INTEGER)
CREATE MEMORY TABLE T_SYSTEM_ROLE_RESOURCE(ROLE_ID INTEGER,RESOURCE_ID INTEGER)
CREATE MEMORY TABLE T_TEMPLATE_INFO(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NAME VARCHAR(50) NOT NULL,TEMPLATE_PATH VARCHAR(100) NOT NULL,HTML_PATH VARCHAR(100) NOT NULL,LAST_DATE TIMESTAMP,DESCRIPTION VARCHAR(100))
CREATE MEMORY TABLE T_TEMPLATE_DATA(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,DATA_NAME VARCHAR(50) NOT NULL,SQL_VALUE VARCHAR(1000) NOT NULL,IS_UNIQUE_RESULT INTEGER,DESCRIPTION VARCHAR(100),TEMPLATE_ID INTEGER NOT NULL)
CREATE MEMORY TABLE T_TASK_INFO(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,TASK_NAME VARCHAR(50) NOT NULL,NEXT_RUN_TIME TIMESTAMP,RUN_UNIT INTEGER,ENABLED INTEGER,INSTANCE_NAME VARCHAR(50) NOT NULL,DESCRIPTION VARCHAR(100))
CREATE MEMORY TABLE T_TASK_RUN_HISTORY(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,TASK_ID INTEGER NOT NULL,RUN_TIME TIMESTAMP,RUN_STATE INTEGER,RUN_TYPE INTEGER,ERROR_INFO VARCHAR(1000))
ALTER TABLE T_SYSTEM_USER_INFO ALTER COLUMN ID RESTART WITH 10
ALTER TABLE T_SYSTEM_ROLE_INFO ALTER COLUMN ID RESTART WITH 5
ALTER TABLE T_SYSTEM_RESOURCE_INFO ALTER COLUMN ID RESTART WITH 10
ALTER TABLE T_TEMPLATE_INFO ALTER COLUMN ID RESTART WITH 0
ALTER TABLE T_TEMPLATE_DATA ALTER COLUMN ID RESTART WITH 0
ALTER TABLE T_TASK_INFO ALTER COLUMN ID RESTART WITH 6
ALTER TABLE T_TASK_RUN_HISTORY ALTER COLUMN ID RESTART WITH 0
